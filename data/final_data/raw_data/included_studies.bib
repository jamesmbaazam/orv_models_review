@Article{Scoglio2010,
  author        = {Scoglio, Caterina and Schumm, Walter and Schumm, Phillip and Easton, Todd and Chowdhury, Sohini Roy and Sydney, Ali and Youssef, Mina},
  journal       = {PLoS ONE},
  title         = {{Efficient mitigation strategies for epidemics in rural regions}},
  year          = {2010},
  issn          = {19326203},
  number        = {7},
  volume        = {5},
  abstract      = {Containing an epidemic at its origin is the most desirable mitigation. Epidemics have often originated in rural areas, with rural communities among the first affected. Disease dynamics in rural regions have received limited attention, and results of general studies cannot be directly applied since population densities and human mobility factors are very different in rural regions from those in cities. We create a network model of a rural community in Kansas, USA, by collecting data on the contact patterns and computing rates of contact among a sampled population. We model the impact of different mitigation strategies detecting closely connected groups of people and frequently visited locations. Within those groups and locations, we compare the effectiveness of random and targeted vaccinations using a Susceptible-Exposed-Infected-Recovered compartmental model on the contact network. Our simulations show that the targeted vaccinations of only 10% of the sampled population reduced the size of the epidemic by 34.5%. Additionally, if 10% of the population visiting one of the most popular locations is randomly vaccinated, the epidemic size is reduced by 19%. Our results suggest a new implementation of a highly effective strategy for targeted vaccinations through the use of popular locations in rural communities. {\textcopyright} 2010 Scoglio et al.},
  address       = {K-State EpiCenter, Department of Electrical and Computer Engineering, Kansas State University, Manhattan, KS, United States Department of Family Studies and Human Sciences, Kansas State University, Manhattan, KS, United States Department of Industrial and},
  annote        = {RAYYAN-INCLUSION: {"elishaare"=>"Included"} | RAYYAN-LABELS: XP,px},
