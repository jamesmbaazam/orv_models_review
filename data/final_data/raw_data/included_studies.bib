  keywords = {Antiviral Agents, Computer simulation, Cost Savings, Cost-benefit analysis, Disease Outbreaks, Economics, Human disease outbreaks, Humans, Influenza, Human, Models, Biological, Economic, Pharmaceutical models, Quality-Adjusted Life Years, Stochastic Processes, Theoretical, United States, antivirus agent, article, biological model, comparative study, controlled study, cost benefit analysis, cost control, cost effectiveness analysis, cost utility analysis, disease severity, disease transmission, economic evaluation, health care cost, human, mortality, oseltamivir, outcome assessment, pandemic, priority journal, procedures, public health, quality adjusted life year, quality of life, statistical model, statistics, stochastic model, vaccination, vaccine},
  language = {English},
