  keywords      = {*Models, Adolescent, Adult, Aged, Antiviral Agents/therapeutic use, Child, Computer Simulation, Disease Outbreaks/*prevention & control, H5N1 Subtype/*physiology, Human, Human/drug therapy/*epidemiology/*preve, Humans, Infant, Influenza, Influenza A Virus, Influenza Vaccines/therapeutic use, Italy, Italy/epidemiology, James, Middle Aged, Preschool, Social Distance, Statistical, Young Adult, reviewed},
  language      = {eng},
  mendeley-tags = {James,reviewed},
  pmid          = {18272019},
  url           = {https://www.cambridge.org/core/services/aop-cambridge-core/content/view/1ED600F564FA3D29547C880D8BF4E398/S095026880800037Xa.pdf/div-class-title-scenarios-of-diffusion-and-control-of-an-influenza-pandemic-in-italy-div.pdf},
}

@Article{Yang2016,
  author        = {Yang, Yali and Tang, Sanyi and Ren, Xiaohong and Zhao, Huiwen and Guo, Chenping},
  journal       = {Discrete and Continuous Dynamical Systems - Series B},
