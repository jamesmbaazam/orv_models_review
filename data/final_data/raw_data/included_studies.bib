  keywords      = {Article, Community Health Centers, Community Health Services, Ebola, Ebola hemorrhagic fever, Ebola virus, Ebolavirus, Hemorrhagic Fever, Humans, James, Models, Sierra Leone, Statistical, clinical feature, community care, epidemic, evaluation study, health center, hospital admission, human, infection risk, nonhuman, outcome assessment, patient care, reviewed, risk assessment, statistical model, transmission, virus transmission},
  language      = {English},
  mendeley-tags = {James,reviewed},
