  isbn          = {9780769544229},
  keywords      = {Computer simulation, Epidemic dynamics, Geographic information system, James, Multilayer model, Travel network, reviewed},
  language      = {English},
  mendeley-tags = {James,reviewed},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-80052150791&doi=10.1109%2FICFCSA.2011.37&partnerID=40&md5=c212f80b013f1cec35409b674f20131f},
