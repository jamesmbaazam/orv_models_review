  url      = {https://link.springer.com/content/pdf/10.1057/jphp.2011.1.pdf},
}

@Article{Gao2016,
  author        = {Gao, Xiaolei and Wei, Jianjian and Cowling, Benjamin J. and Li, Yuguo},
  journal       = {Science of the Total Environment},
