  keywords  = {Algorithms, Ebolavirus, Extinction, Biological, Hemorrhagic Fever, Ebola, Humans, Liberia, Models, Theoretical, Population Dynamics, algorithm, human, population dynamics, species extinction, theoretical model, transmission},
  language  = {English},
