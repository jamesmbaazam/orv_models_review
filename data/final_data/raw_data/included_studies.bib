  keywords      = {*Models, *Population Surveillance, Cholera/*epidemiology/prevention & control/*transm, Epidemics/*prevention & control, Haiti/epidemiology, Humans, James, Statistical, reviewed},
  language      = {eng},
  mendeley-tags = {James,reviewed},
