  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84923173887&doi=10.3201%2Feid2103.141892&partnerID=40&md5=4537772d7963ffc5a42de5e7d88ea2af     https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4344293/pdf/14-1892.pdf},
}

@Article{Aldila2013,
  author        = {Aldila, Dipo and G{\"{o}}tz, Thomas and Soewono, Edy},
  journal       = {Mathematical Biosciences},
  title         = {{An optimal control problem arising from a dengue disease transmission model}},
