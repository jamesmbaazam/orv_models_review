@Article{Saito2013,
  author        = {Saito, Masaya M. and Imoto, Seiya and Yamaguchi, Rui and Tsubokura, Masaharu and Kami, Masahiro and Nakada, Haruka and Sato, Hiroki and Miyano, Satoru and Higuchi, Tomoyuki},
  journal       = {PLoS ONE},
  title         = {{Enhancement of Collective Immunity in Tokyo Metropolitan Area by Selective Vaccination against an Emerging Influenza Pandemic}},
