  keywords  = {Diseases, Epidemic model, Epidemic modeling, Health Policy, Infectious disease, Meta-population model, Migration, Population model, Public health, Public health policies, Public health policy, Stochastic models, Stochastic systems, Vaccination, Vaccines},
  language  = {English},
